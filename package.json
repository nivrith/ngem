{
  "name": "@polyrithm/ngen",
  "description": "CLI Utility tool for Angular and AngularJS Development",
  "version": "0.0.23",
  "author": "Nivrith",
  "bin": {
    "ngen": "./bin/run"
  },
  "bugs": "https://github.com/Polyrithm/ngen/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-commands": "^1.2.2",
    "@oclif/plugin-help": "^2.1.3",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-update": "^1.3.4",
    "@oclif/plugin-warn-if-update-available": "^1.5.4",
    "@types/figlet": "^1.2.0",
    "@types/handlebars": "^4.0.39",
    "@types/node-notifier": "^0.0.28",
    "@types/opn": "^5.1.0",
    "case": "^1.5.5",
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "figlet": "^1.2.1",
    "handlebars": "^4.0.12",
    "is-online": "^7.0.0",
    "node-notifier": "^5.2.1",
    "opn": "^5.4.0",
    "simple-git": "^3.5.0",
    "ts-simple-ast": "^17.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "gulp": "^3.9.1",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "yarn.lock"
  ],
  "homepage": "https://github.com/Polyrithm/ngen",
  "keywords": [
    "angularjs",
    "cli",
    "oclif",
    "utility"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ngen",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available"
    ],
    "macos": {
      "identifier": "com.ngen.cli"
    }
  },
  "repository": "Polyrithm/ngen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && gulp copy:templates && gulp copy:images && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
